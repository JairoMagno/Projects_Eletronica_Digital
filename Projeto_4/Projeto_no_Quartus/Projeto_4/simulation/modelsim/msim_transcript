add wave -position insertpoint  \
sim:/gerar_todos_pulsos/mute_pulso_R \
sim:/gerar_todos_pulsos/mute_remoto \
sim:/gerar_todos_pulsos/play_local \
sim:/gerar_todos_pulsos/play_pulso_L \
sim:/gerar_todos_pulsos/play_pulso_R \
sim:/gerar_todos_pulsos/play_remoto \
sim:/gerar_todos_pulsos/selecao_local \
sim:/gerar_todos_pulsos/selecao_pulso_L \
sim:/gerar_todos_pulsos/selecao_pulso_R \
