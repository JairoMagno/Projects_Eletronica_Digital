sim:/gerar_todos_pulsos/stop_local \
sim:/gerar_todos_pulsos/stop_pulso_L \
sim:/gerar_todos_pulsos/stop_pulso_R \
sim:/gerar_todos_pulsos/stop_remoto
add wave -position insertpoint  \
sim:/gerar_todos_pulsos/clock_in
force -freeze sim:/gerar_todos_pulsos/clock_in 1 0, 0 {50 ps} -r 100
force -freeze sim:/gerar_todos_pulsos/play_local 1 0
force -freeze sim:/gerar_todos_pulsos/selecao_local 1 0
force -freeze sim:/gerar_todos_pulsos/stop_local 1 0
force -freeze sim:/gerar_todos_pulsos/mute_remoto 1 0
force -freeze sim:/gerar_todos_pulsos/play_remoto 1 0
force -freeze sim:/gerar_todos_pulsos/selecao_remoto 1 0
force -freeze sim:/gerar_todos_pulsos/stop_remoto 1 0
run
run
run
